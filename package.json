{
  "name": "flag-guesser-api",
  "version": "1.0.0",
  "description": "REST API for Flag Guesser Game Platform",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229 src/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:seed": "node scripts/seed-test-data.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "2.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.2.3",
    "redis": "^4.6.11",
    "sharp": "^0.33.0",
    "socket.io": "^4.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
